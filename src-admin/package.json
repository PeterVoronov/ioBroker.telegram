        "craco-esbuild": "^0.6.1",
        "craco-module-federation": "^1.1.0",
        "prop-types": "^15.8.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-qr-code": "^2.0.15",
        "react-refresh": "^0.14.2",
        "react-scripts": "^5.0.1"
