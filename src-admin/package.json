        "vite-plugin-svgr": "^4.3.0",
        "vite-tsconfig-paths": "^5.1.4",        
        "@vitejs/plugin-react": "^4.3.4",
        "@craco/craco": "^7.1.0",
        "@iobroker/adapter-react-v5": "^7.4.3",
        "@iobroker/json-config": "^7.4.3",
        "@mui/icons-material": "^5.16.1",
