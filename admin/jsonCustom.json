  "items": {
    "alias": {
      "type": "text",
      "sm": 12,
      "label": "Alias",
      "default": "",
      "help": "Leave empty to use object's name"
    },
    "readOnly": {
      "newLine": true,
      "type": "checkbox",
      "label": "Read only",
      "help": "Only delivers the changes",
      "defaultFunc": "customObj.common && customObj.common.write === false",
      "sm": 6
    },
    "report": {
      "type": "checkbox",
      "label": "Report changes",
      "help": "Sends a new message if the value changes",
      "defaultFunc": "customObj.common && customObj.common.read !== false",
      "sm": 6
    },
    "buttons": {
      "newLine": true,
      "type": "number",
      "label": "Buttons in line",
      "default": 3,
      "min": 1,
      "max": 3,
      "sm": 6
    },
    "writeOnly": {
      "type": "checkbox",
      "label": "Write only",
      "default": false,
      "help": "No status query",
      "sm": 6
    },
    "offCommand": {
      "newLine": true,
      "type": "text",
      "hidden": "customObj.common.type !== 'boolean' || data.onlyTrue",
      "label": "OFF command",
      "help": "Default - 'OFF'",
      "default": "",
      "hideOnlyControl": true,
      "sm": 6
    },
    "onCommand": {
      "type": "text",
      "hidden": "customObj.common.type !== 'boolean' || data.onlyFalse",
      "label": "ON command",
      "help": "Default - 'ON'",
      "default": "",
      "sm": 6
    },
    "offStatus": {
      "newLine": true,
      "hidden": "customObj.common.type !== 'boolean' || data.onlyTrue",
      "hideOnlyControl": true,
      "type": "text",
      "label": "OFF status",
      "help": "Default - 'OFF'",
      "default": "",
      "sm": 6
    },
    "onStatus": {
      "type": "text",
      "hidden": "customObj.common.type !== 'boolean' || data.onlyFalse",
      "label": "ON status",
      "help": "Default - 'ON'",
      "default": "",
      "sm": 6
    },
    "onlyFalse": {
      "newLine": true,
      "type": "checkbox",
      "hidden": "customObj.common.type !== 'boolean'",
      "disabled": "data.onlyTrue",
      "label": "Only false",
      "help": "Do not show ON",
      "default": false,
      "sm": 6
    },
    "onlyTrue": {
      "hidden": "customObj.common.type !== 'boolean'",
      "disabled": "data.onlyFalse",
      "defaultFunc": "customObj.common ? !!(customObj.common.role || '').match(/^button/) : false",
      "type": "checkbox",
      "label": "Only true",
      "help": "Do not show OFF",
      "sm": 6
    }
  }
}